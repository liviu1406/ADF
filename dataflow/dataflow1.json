{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "MoviesADLS"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "RankMoviesByRatings"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Movie as string,",
				"          Title as string,",
				"          Genres as string,",
				"          Year as integer,",
				"          Rating as string,",
				"          {Rotten Tomato} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> MoviesADLS",
				"MoviesADLS filter(Year > 1950) ~> filter1",
				"filter1 derive(PrimaryGenre = iif(locate('|', Genres) > 1, left(Genres, locate('|',Genres)-1),Genres)) ~> derivedColumn1",
				"derivedColumn1 window(over(PrimaryGenre,",
				"          Year),",
				"     asc({Rotten Tomato}, true),",
				"     RatingsRank = rank()) ~> RankMoviesByRatings"
			]
		}
	}
}